    {
      "name": "Profile",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "framework/lib/main.dart",
      "args": [
        "--dart-define-from-file",
        "env/staging.json"
      ]
    },
  ]
}